    "mongodb": "^5.7.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
